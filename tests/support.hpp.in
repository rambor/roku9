#include "gtest/gtest.h"

namespace tests {
static std::string fixture(const std::string & filename){
    std::string name ="@CMAKE_SOURCE_DIR@/tests/fixtures/" + filename;
    std::cout << "TEST loading :: " << name << std::endl;
    return name;
}

static std::string loadInp(const std::string & filename){
    std::string name ="@CMAKE_BINARY_DIR@/tests/" + filename;
    std::cout << "TEST loading :: " << name << std::endl;
    return name;
}
}

